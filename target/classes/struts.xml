<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>



 <constant name="struts.i18n.encoding" value="utf-8"/>
     <constant name="struts.devMode" value="true" />
     <!--Struts2中Action对象的创建交给spring完成-->
     <constant name="struts.objectFactory" value="spring" />

	 <package name="user" namespace="/user" extends="struts-default">
	        <action name="user_*" class="userAction" method="{1}">
	        	<result name="show_view">/index.jsp</result>
	        	<result name="input">/login.jsp</result>
	        </action>
	 </package>
	 
	  <package name="garbage" namespace="/garbage" extends="struts-default">
	        <action name="garbage_*" class="garbageAction" method="{1}">
	        	<result name="show_view">/index.jsp</result>
	        	<result name="baike">/baike.jsp</result>
	        </action>
	 </package>
	 
	 <package name="garbagetype" namespace="/garbagetype" extends="struts-default">
	        <action name="garbagetype_*" class="garbagetypeAction" method="{1}">
	        	
	        	<result name="baike">
	     			/baike.jsp
     			</result>
     			
     			<result name="show_view">/index.jsp</result>
	        </action>
	 </package>

</struts>
